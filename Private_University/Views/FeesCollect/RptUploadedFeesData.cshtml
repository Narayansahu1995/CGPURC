@model IEnumerable<Private_University.Models.Uploaded_data_files>
@using Private_University.Models;
@{
    ViewBag.Title = "RptUploadedFeesData";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<style type="text/css">
    .Delete_Img {
        background: url('data:image/png;base64,') no-repeat;
        display: block;
        height: 34px;
        text-indent: -9999px; /* hides the link text */
    }
</style>
<script type="text/javascript">
    function Delete(i,FP) {
        debugger;
        var FID = 0;
        var FiP = "";
        FID = parseInt(i);
        FiP = "~" + FP;
        //alert(FiP);
        $.post("DeleteFeesFile", { File_Id: FID, FPath: FiP }, function (data) {
            alert(data);
        });
        location.replace("RptUploadedFeesData");
    }
</script>

<div class="sb2-2-3">
    <div class="row">
        <div class="col-md-12">
            <div class="box-inn-sp admin-form">
                <div class="inn-title">
                    <h4>Uploaded Fees data for process</h4>
                </div>
                <div class="tab-inn">

                       
                        <div class="form-horizontal">                          
                           

                            <table class="table table-bordered" style="width:auto;margin:auto;" id="Flist">
                                <thead>
                                    <tr>
                                        <th>SNo </th>
                                        <th>University Name</th>
                                        <th>File Name</th>
                                        <th>Uploaded Date</th>                                      
                                        @*<th>Processed</th>
                                         <th>Records Exported to server</th>
                                         <th>Records Left</th>
                                         <th>Remark</th>*@
                                        <th>Download</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model.Count() != 0 || Model != null)
                                    {
                                        foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.count)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.University_Name)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.File_Name)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Uploaded_DateTime)
                                                </td>
                                                @*<td>
                                                    @Html.DisplayFor(modelItem => item.Processed)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Records_exported_to_Server)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Records_left)
                                                </td>

                                                <td>
                                                    @Html.DisplayFor(modelItem => item.Remark)
                                                </td>*@
                                                <td style="text-align: center;">
                                                    <a href="@Html.DisplayFor(modelItem => item.File_path).ToString()">
                                                        <img id="DW" runat="server" alt='DWbtn' src='data:image/png;base64,'
                                                             height='30' style='cursor: pointer;' />
                                                    </a>
                                                </td>
                                                <td class='File_ID'>
                                                    <a class='Delete_Img' href='javascript:;' onclick="Delete('@item.File_ID', '@item.File_path')">Delete</a>
                                                </td>

                                            </tr>
                                        }
                                    }
                                    else
                                    {
                                        <tr>
                                            <td colspan="4" style="text-align:center;">
                                                No Records Found
                                            </td>
                                        </tr>
                                    }

                                </tbody>
                            </table>

                        </div>
                    
                </div>
            </div>
        </div>
    </div>
</div>
