<!--SECTION START-->
<section class="pop-cour">
    <div class="container com-sp pad-bot-70">
        <div class="row">
            <div class="cor about-sp">
                <div class="ed-about-tit">
                    <div class="con-title">
                        <h2><span> @Html.Label(Private_University.Resources.Objective.Objectives, new { @class = "con-title-h2-span" })</span></h2>
                        <p>@Html.Label(Private_University.Resources.Objective.CommName, new { @class = "con-title-p" }) </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="cor-p4">
                    <h3>@Html.Label(Private_University.Resources.Objective.Title, new { @class = "con-title-p", @style = "color: #203245;font-size: 24px;" }) </h3>
                    <p>
                        <ul class="list text-justify">
                            <li>
                                <i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointOne
                            </li>
                            <li><i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointTwo
                            </li>
                            <li>
                                <i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointThree
                            </li>
                            <li>
                                <i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointFour
                            </li>
                            <li>
                                <i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointFive
                            </li>
                            <li>
                                <i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointSix
                            </li>
                            <li>
                                <i class="fa fa-angle-right"></i> @Private_University.Resources.Objective.PointSeven
                            </li>
                        </ul>
                    </p>
                </div>
            </div>
        </div>
    </div>
</section>
