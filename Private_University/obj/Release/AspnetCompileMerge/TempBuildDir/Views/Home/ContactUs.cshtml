@model Private_University.Models.CommissionAddressUpdate
@using Private_University.Models;
@{
    ViewBag.Title = "UpdateAddress";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!--SECTION START-->
<section class="pop-cour">
    <div class="container com-sp pad-bot-70">
        <div class="row">
            <div class="cor about-sp">
                <div class="ed-about-tit">
                    <div class="con-title">
                        <h2>@Html.Label(Private_University.Resources.AddressContact.Contact, new { @class = "con-title-h2" })<span> @Html.Label(Private_University.Resources.AddressContact.Us, new { @class = "con-title-h2-span" })</span></h2>
                    </div>
                </div>
            </div>
        </div>
        @*<div class="row">*@
            <div class="col-md-8 col-xs-12">
                <div class="cor-p4">
                    <h3>@Html.Label(Private_University.Resources.AddressContact.CommiName, new { @class = "con-title-p", @style = "color: #203245;font-size: 24px;" }) </h3>
                    <p>
                        <span><i class="fa fa-home"></i> </span><label><span> </span>@Html.Label(Private_University.Resources.AddressContact.Address, new { @class = "con-p" }) <strong>@ViewBag.Addressss</strong></label>
                    </p>
                    <p>
                        <span><i class="fa fa-phone"></i> </span><label><span> </span>@Html.Label(Private_University.Resources.AddressContact.Contact, new { @class = "con-p" }) <strong>@ViewBag.Contact_No</strong> </label>
                    </p>
                    <p>
                        <span><i class="fa fa-envelope"></i> </span><label><span> </span>@Html.Label(Private_University.Resources.AddressContact.Email, new { @class = "con-p" }) <strong>@ViewBag.Email_ID</strong> </label>
                    </p>
                </div>
            </div>
            <div class="col-md-4  col-xs-12">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d399.9502429828399!2d81.77006492417541!3d21.16479159600874!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3a28c41af961fe59%3A0x26c6d29849805b80!2sOffice%20Campus!5e0!3m2!1sen!2sin!4v1646984030234!5m2!1sen!2sin" style="border:0; width: 100%; height: 350px;" allowfullscreen="" loading="lazy" frameborder="2"></iframe>
            </div>
