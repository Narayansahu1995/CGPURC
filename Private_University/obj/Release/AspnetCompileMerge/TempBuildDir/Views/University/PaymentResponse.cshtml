
@{
	ViewBag.Title = "Payment Status";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="pop-cour">
	<div class="container com-sp pad-bot-70">
		<div class="row">
			<div class="cor about-sp">
				<div class="ed-about-tit">
					<div class="con-title">
						<h2>
							<label class="con-title-h2">Payment</label>
							<span>
								<label class="con-title-h2-span">Response</label>
							</span>
						</h2>
						<p>
							Payment status that has done recently
						</p>
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12">
				<div class="cor-p4">
					<p> Payment Transaction has been @ViewBag.TxnStatus </p>
					<p>Transaction Number : @ViewBag.TxnNumber</p>
					<p>Bank Transaction ID : @ViewBag.TxnBankNo</p>
					<p>Transaction Amount in Rupee : @ViewBag.TxnAmt</p>
				</div>
			</div>
		</div>
	</div>
</section>


