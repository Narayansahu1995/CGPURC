    <title>@ViewBag.Title - Chhattisgarh Private Universities Regulatory Commission</title>

    @*<link href="~/Content/Site.css" rel="stylesheet" type="text/css" />            
        <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />*@

    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/css/materialize.css" rel="stylesheet" type="text/css">
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/MyCustom.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/style.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/style-mob.css" rel="stylesheet" type="text/css" />
