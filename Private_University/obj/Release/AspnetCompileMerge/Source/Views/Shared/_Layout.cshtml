                                </li>
                            </ul>
                        </div>
                        <div class="ed-com-t1-right">
                            <ul>
                                <li>
                                    @Html.ActionLink(Private_University.Resources.Global.SignIn, "Login", "Home")
                                </li>
                                <li>
