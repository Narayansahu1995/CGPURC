                                </li>
                                <li><img src="~/Content/images/icon/english_lang.png" />@Html.ActionLink("English", "Index", "Language", new { LanguageAbbrevation = "en" }, null)</li>
                                <li><img src="~/Content/images/icon/hindi_lang.png" />@Html.ActionLink("हिंदी", "Index", "Language", new { LanguageAbbrevation = "Hi" }, null)</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="wed-logo">
                            <a href="~/Home/Index">
                                <img src="~/Content/images/purc_white_logo.png" class="img-circle" alt="" />
                            </a>
                            <div class="logo-text">
                                @*<h2>Chhattisgarh Private University Regulatory Commission</h2>
                                    <h4> Sector-24, Block 7B, Ground Floor, Nawa Raipur Atal Nagar, Raipur,Chhattisgarh</h4>*@
                                <h2>@Html.Label(Private_University.Resources.Global.DepartmentName, new { @class = "DepartmentName" })</h2>
                                <h4>@Html.Label(Private_University.Resources.Global.Address, new { @class = "Address" })</h4>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- LOGO AND MENU SECTION -->
        <div class="top-logo" data-spy="affix" data-offset-top="250">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="main-menu">
                            <ul>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Home, "Index", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.AboutUs, "AboutUs", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.OfficeBearer, "OfficeBearers", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Universities, "Univsersities", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.ActRulesAndRegulations, "RulesRegulations", "Home")</li>
                                @*<li>@Html.ActionLink(Private_University.Resources.Global.Notifications, "Notifications", "Home")</li>*@
                                <li>@Html.ActionLink(Private_University.Resources.Global.Rti, "RTI", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Gallery, "Gallery", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.News, "News", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Contact, "ContactUs", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Feedback, "Feedback", "Home")</li>
                            </ul>
                        </div>
                    </div>
                    <div class="all-drop-down-menu">
                    </div>
                </div>
            </div>
        </div>

    </section>
    <!--END HEADER SECTION-->
    @RenderBody()
