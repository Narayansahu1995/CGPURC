                                <li>@Html.ActionLink(Private_University.Resources.Global.Home, "Index", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.AboutUs, "AboutUs", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.OfficeBearer, "OfficeBearers", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Universities, "Univsersities", "Home")</li> 
                                <li>@Html.ActionLink(Private_University.Resources.Global.ActRulesAndRegulations, "RulesRegulations", "Home")</li>
                                
                                @*<li>@Html.ActionLink(Private_University.Resources.Global.Notifications, "Notifications", "Home")</li>*@
                                <li>@Html.ActionLink(Private_University.Resources.Global.Rti, "RTI", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Gallery, "Gallery", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.News, "News", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Contact, "ContactUs", "Home")</li>
                                <li>@Html.ActionLink(Private_University.Resources.Global.Feedback, "Feedback", "Home")</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--HEADER SECTION-->
