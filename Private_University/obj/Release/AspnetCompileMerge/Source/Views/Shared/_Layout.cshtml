    <!-- COPY RIGHTS -->
    <section class="wed-rights">
        <div class="container">
            <div class="row">
                <div class="copy-right">
                    <a href="http://chips.gov.in/" target="_blank">
                        <img class="text-center img img-responsive" style="margin:auto;" src="~/Content/images/chips.png" height="20" />
                        <p>
                            @Html.Label(Private_University.Resources.Global.FooterCopyright, new { @class = "Footer-p" }) © @System.DateTime.Now.Year.ToString().@Html.Label(Private_University.Resources.Global.FooterCopyName, new { @class = "Footer-p" })
