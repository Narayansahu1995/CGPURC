    @*<script src="~/Scripts/modernizr-2.8.3.js"></script>*@
    <style>
        /*map css start*/
        .highlight {
            fill: #ec008c;
            opacity: 1;
        }

        .show {
            /* transform: rotate(-10deg) scale(1.05); */
            transition: transform 0.3s ease;
            transform: rotate(-1.2deg);
            text-align: center;
            filter: drop-shadow(0.25em 0.7em 0.95em rgba(0, 0, 0, 0.8));
        }
        /*map css end*/

        /*layout custom css start*/
        .email, .phone {
            color: #a3adb9;
            font-size: 12px;
        }

        .DepartmentName {
            font-weight: 700;
            line-height: 28px;
            font-size: 30px;
            margin-top: 0px;
            margin-bottom: 10px;
            color: white;
        }

        .Address {
            font-weight: 600;
            line-height: 24px;
            font-size: 18px;
            margin-top: 0px;
            margin-bottom: 0px;
            color: white;
        }

        .Footer-h4 {
            font-size: 14px;
            color: #d6d6d6;
            text-transform: uppercase;
            font-weight: 600;
        }

        .Footer-p {
            font-size: 13px;
            color: #fff;
            line-height: 18px;
        }
        /*layout custom css end*/
    </style>
    <script type="text/javascript">
    </script>
</head>
<body>
    <!-- MOBILE MENU -->
